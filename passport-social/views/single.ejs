										<li><strong>Language:</strong> <%=movie.Language%></li>
										<li><strong>Premiere:</strong> <%=movie.Released%>(<%=movie.Country%>)</li>
										<li><strong>Category:</strong> <%=movie.Genre%>-<%=movie.Type%></li>
									</ul>

									<ul class="starring">
										<li><strong>Directors:</strong> <%=movie.Director%></li>
										<li><strong>Writers:</strong> <%=movie.Writer%></li>
										<li><strong>Stars:</strong> <%=movie.Actors%></li>
									</ul>
